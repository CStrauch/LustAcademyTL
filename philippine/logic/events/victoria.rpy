    # ch_Name "{i}(It's about time I showed the power Leona gave me!){/i}"
    ch_Name "{i}(Panahon na para ipakita ko ang kapangyarihang ibinigay sa akin ni Leona!){/i}"

# game/logic/events/victoria.rpy:558
