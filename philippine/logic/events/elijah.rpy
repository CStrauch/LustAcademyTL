    # ch_Name "Oh, wow..."
    ch_Name ""

# game/logic/events/elijah.rpy:529
translate philippine Elijah_4_label_28_8e644f05:

    # ch_Name "All right, I'm on it."
    ch_Name "Sige, ako na ang bahala."

# game/logic/events/elijah.rpy:530
