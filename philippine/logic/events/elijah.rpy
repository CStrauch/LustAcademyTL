    Elijah "Walang kahiyaan, walang duda. Kasiyahan."

# game/logic/events/elijah.rpy:523
translate philippine Elijah_4_label_a0bf5e64:

    # ch_Name "What is that, a drug?"
    ch_Name "Ano ito, droga?"

